{
    "manifestVersion": 1,
    "id": "CosmosDbThroughputScalerExtension",
    "name": "Cosmos DB Throughput Scaler",
    "version": "1.0.19",
    "publisher": "williamoconnor",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Scale Cosmos DB RU throughput",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon-square.png"
    },
    "tags": [
        "releasemanagement",
        "cosmos",
        "az"
    ],
    "files": [
        {
            "path": "buildAndReleaseTask"
        },
        {
            "path": "images/settings.png",
            "addressable": "true"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "branding": {
        "theme": "light",
        "color": "rgb(128, 128, 192)"
    },
    "screenshots": [
        {
            "path": "images/settings.png"
        }
    ],
    "links": {
        "license": {
            "uri": "https://github.com/williamoconnorme/AzureDevOpsCosmosDbScaleTask/blob/master/license.txt"
        },
        "issues": {
            "uri": "https://github.com/williamoconnorme/AzureDevOpsCosmosDbScaleTask/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/williamoconnorme/AzureDevOpsCosmosDbScaleTask"
    },

    "contributions": [
        {
            "id": "me.williamoconnor.CosmosDbThroughputScalerExtension",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}